{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://property-rental-six.vercel.app/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://property-rental-six.vercel.app"
        }
      ]
    }
  ]
}

  